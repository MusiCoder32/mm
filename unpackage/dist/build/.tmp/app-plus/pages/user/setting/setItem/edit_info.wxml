<view><view class="content"><view class="row"><view class="nominal">昵称</view><view class="input"><input focus="true" placeholder="请输入新昵称" type="text" data-event-opts="{{[['input',[['__set_model',['','nickname','$event',[]]]]]]}}" value="{{nickname}}" bindinput="__e"/></view></view><view class="row"><view class="nominal">性别</view><view><radio-group data-event-opts="{{[['change',[['radioChange',['$event']]]]]}}" style="padding-top:24rpx;" bindchange="__e"><label class="set-radio"><radio name="gender" value="0" checked="{{gender==0}}"></radio>保密</label><label class="set-radio"><radio name="gender" value="1" checked="{{gender==1}}"></radio>男</label><label class="set-radio"><radio name="gender" value="2" checked="{{gender==2}}"></radio>女</label></radio-group></view></view></view><view data-event-opts="{{[['tap',[['save',['$event']]]]]}}" class="save" bindtap="__e"><view class="btn">保存修改</view></view></view>